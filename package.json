{
  "name": "inbody-analyzer",
  "module": "index.ts",
  "type": "module",
  "private": true,
  "devDependencies": {
    "@types/bun": "latest",
    "drizzle-kit": "^0.31.9"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "scripts": {
    "dev": "bun run --hot src/index.ts",
    "db:migrate": "bun run src/db/migrate.ts",
    "db:seed": "bun run src/db/seed.ts",
    "db:reset": "rm -f ./data/inbody.db && bun run db:migrate && bun run db:seed"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.74.0",
    "@hono/node-server": "^1.19.9",
    "drizzle-orm": "^0.45.1",
    "heic-convert": "^2.1.0",
    "hono": "^4.11.9"
  }
}
